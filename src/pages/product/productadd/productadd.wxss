page {
    background-color: #f4f4f4;
    box-sizing: border-box;
}

.t-itme {
    height: 91rpx;
    width: 100%;
    display: flex;
    justify-content: first baseline;
    border: 1rpx solid #eeeeee;
    background-color: #ffffff;
}

.t-itme:nth-child(2) {
    border-top: none;
    border-bottom: 1rpx solid #eeeeee;
}

.t-itme .t-itmeName {
    font-size: 32rpx;
    width: 26%;
    height: 100%;
    line-height: 91rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
}

.t-itme .t-itmeText {
    width: 74%;
    height: 100%;
    font-size: 30rpx;
}

.t-itmeText input {
    height: 100%;
    line-height: 91rpx;
}

.t-itmeBox {
    height: 134rpx;
    width: 100%;
    border-bottom: 1rpx solid #eeeeee;
    display: flex;
    justify-content: space-around;
    background-color: #ffffff;
}

.t-itmeBox .t-itmePrice {
    font-size: 30rpx;
    color: #999999;
    width: 50%;
    margin-left: 30rpx;
    margin-top: 23rpx;
}

.t-itmeBox .t-itmeDiscount {
    font-size: 30rpx;
