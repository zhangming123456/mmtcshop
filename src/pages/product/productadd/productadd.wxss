    width: 50%;
    margin-left: 30rpx;
    margin-top: 23rpx;
}

.t-itmeMoney {
    color: #ff8000;
    font-size: 32rpx;
    margin-top: 29rpx;
}

.border-r {
    position: relative;
}

.border-r:after {
    content: ' ';
    position: absolute;
    left: auto;
    background: #ddd;
    right: 0;
    top: 3rpx;
    height: 100%;
    bottom: 0;
    width: 2px;
    height: 90rpx;
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
}

