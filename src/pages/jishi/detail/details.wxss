/* 技师头像 */

.head {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 91rpx;
  width: 100%;
  position: relative;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
}

.enameHead {
  width: 25%;
  height: 100%;
}

.enameHead text {
  font-size: 32rpx;
  line-height: 91rpx;
  margin-left: 30rpx;
}

.headBox {
  width: 75%;
  height: 100%;
}

.headBox .picture {
  width: 71rpx;
  height: 71rpx;
  border-radius: 50%;
  position: absolute;
  right: 76rpx;
  top: 10rpx;
}

.headBox .arrows {
  width: 15rpx;
  height: 25rpx;
  position: absolute;
  right: 30rpx;
  top: 33rpx;
}

