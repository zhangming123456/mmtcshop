/* 技师管理 */

.name {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 91rpx;
  width: 100%;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
}

.ename {
  width: 25%;
  height: 100%;
}

.ename text {
  font-size: 32rpx;
  line-height: 91rpx;
  margin-left: 30rpx;
}

.input {
  width: 75%;
  height: 100%;
  font-size: 30rpx;
}

.input input {
  height: 100%;
  line-height: 91rpx;
}

