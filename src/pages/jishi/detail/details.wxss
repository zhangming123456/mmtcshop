/* 擅长的领域 */

.skilled {
  height: 160rpx;
  width: 100%;
  margin-top: 24rpx;
  background-color: #fff;
  position: relative;
  /* top: 24rpx; */
}

.skilledBox {
  width: 100%;
  position: absolute;
}

.skilledText {
  position: absolute;
  font-size: 32rpx;
  left: 30rpx;
  top: 30rpx;
}

.skilledBox .number {
  position: absolute;
  font-size: 30rpx;
  color: #999;
  right: 29rpx;
  top: 32rpx;
}

.skilled .add {
  position: absolute;
  width: 86rpx;
  height: 41rpx;
  border: 1px solid #ff8000;
  top: 90rpx;
  left: 31rpx;
  color: #ff8000;
  text-align: center;
  line-height: 41rpx;
}

.addContentCon {
  position: absolute;
  top: 90rpx;
  left: 127rpx;
  height: 41rpx;
  display: flex;
}

.skilled .addContent {
   position: relative; 
  /* width: 150rpx; */
  height: 41rpx;
  padding: 0 10rpx;
  font-size: 28rpx;
  border: 1px solid #ff8000;
  color: #ff8000;
  text-align: center;
  line-height: 41rpx;
   /* top: 90rpx;
  left: 147rpx; */
   margin-left: 20rpx;  
}

.addContent .cancel {
  position: absolute;
  width: 29rpx;
  height: 29rpx;
  top: -14rpx;
  right: -15rpx;
}

